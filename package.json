{
  "name": "sudoku-core",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest --preset=ts-jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brooklyn-Dev/sudoku-core.git"
  },
  "keywords": [
    "typescript",
    "npm-package",
    "sudoku-solver",
    "sudoku",
    "sudoku-generator",
    "sudoku-board"
  ],
  "author": "Brooklyn Baylis",
  "license": "ISC",
  "description": "A TypeScript library for Sudoku logic; comprised of 3 modules: Board, Solver and Generator.",
  "bugs": {
    "url": "https://github.com/Brooklyn-Dev/sudoku-core/issues"
  },
  "homepage": "https://github.com/Brooklyn-Dev/sudoku-core#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
